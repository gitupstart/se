CREATE TABLE executives (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(100) NOT NULL,
    meeting_time_mon INT DEFAULT 0,
    meeting_time_tue INT DEFAULT 0,
    meeting_time_wed INT DEFAULT 0,
    meeting_time_thu INT DEFAULT 0,
    meeting_time_fri INT DEFAULT 0,
    meeting_time_sat INT DEFAULT 0,
    meeting_time_sun INT DEFAULT 0
);

INSERT INTO executives (name, meeting_time_mon, meeting_time_tue, meeting_time_wed, meeting_time_thu, meeting_time_fri, meeting_time_sat, meeting_time_sun)
VALUES
('John Doe', 4, 6, 5, 7, 6, 8, 5),
('Jane Smith', 5, 5, 6, 4, 7, 5, 6),
('Mike Johnson', 6, 4, 7, 5, 6, 4, 5);
